{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Archiver",
      "type": "debugpy",
      "request": "launch",
      "module": "gtfs_rt_archiver",
      "cwd": "${workspaceFolder}",
      "env": {
        "STORAGE_EMULATOR_HOST": "http://localhost:4443",
        "CONFIG_PATH": "${workspaceFolder}/agencies.yaml",
        "LOG_FORMAT": "text"
      },
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "Start fake-gcs",
      "console": "integratedTerminal"
    }
  ]
}
